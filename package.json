{
  "name": "star",
  "version": "0.0.5",
  "description": "Support for your clinical radiological workflow.",
  "author": "stilbruch.dev",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Stilbruch-Development/Radtoolbox.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stilbruch-Development/Radtoolbox/issues"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "StaR - Standards der Radiologie",
    "extraResources": [
      "./express_server/**/*",
      ".env"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "dexie": "^2.0.4",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.4",
    "electron-is-dev": "^1.1.0",
    "immutable": "^4.0.0-rc.12",
    "material-table": "^1.54.2",
    "npm-check-updates": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "^3.3.0",
    "styled-components": "^4.4.1",
    "validator": "^12.1.0",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "electron": "^7.1.8",
    "electron-builder": "^21.2.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest-dom": "^4.0.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5"
  },
  "proxy": "http://localhost:2000"
}
