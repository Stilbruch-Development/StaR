{
  "name": "star",
  "version": "0.0.8",
  "description": "Support for your clinical radiological workflow.",
  "author": "visionx.dev",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/VisionX-Development/StaR.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VisionX-Development/StaR/issues"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "StaR - Standards der Radiologie",
    "productName": "StaR - Standards der Radiologie",
    "extraResources": [
      ".env",
      "./src/images/StaR_Main_500.svg",
      "./src/fonts/Play/Play-Regular.ttf"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "VisionX-Development",
        "repo": "StaR"
      }
    ],
    "copyright": "Copyright Â© 2020 visionx.dev",
    "mac": {
      "icon": "./src/images/icons/mac/icon.icns",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./src/images/icons/win/icon.ico"
    },
    "files": [
      "./src/components/images/icons**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.7",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.4",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "material-table": "^1.68.0",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "react": "^16.13.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.8.1",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.2.0",
    "uuid": "^8.3.0",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder build --publish never",
    "deploy": "electron-builder build --win --publish always",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.4",
    "babel-eslint": "^10.1.0",
    "electron": "8.5.1",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.1",
    "jest-styled-components": "^7.0.3",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "^2.1.1",
    "react-app-rewired": "^2.1.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}